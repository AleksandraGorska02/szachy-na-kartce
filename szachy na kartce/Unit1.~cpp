void __fastcall TForm1::p1Click(TObject *Sender)
{
        if(pp1=='n'){
          if(kto=='o'){
                p1->Picture->LoadFromFile("img/o.bmp");
                pp1='o';
                kto='x';
                tura->Picture->LoadFromFile("img/x.bmp");
                i++;

          }
          else{
          p1->Picture->LoadFromFile("img/x.bmp");
                pp1='x';
                kto='o';
                tura->Picture->LoadFromFile("img/o.bmp");
                i++  ;


          }  p1->Enabled=false;
          sprawdz();

        }
}
